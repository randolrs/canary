
<% unless user_signed_in? %>

	<%# render 'pages/basic_landing_page' %>

	<%= render 'pages/single_page_landing' %>

<%else%>

	<% if current_user.is_artist %>

		<%= render 'pages/user/dashboard_side_panel' %>

		<div class="dashboard-container">

			<%= render 'pages/user/dashboard_header' %>

			<%= render 'pages/user/dashboard_fixed_header' %>

			<div id="sales" style="display:none;">
				<%= render 'pages/user/balance' %>
			</div>

			<div id="portfolio" style="display:none;">
				<%= render 'pages/user/my_work' %>
			</div>

			<div id="submissions">
				<%= render 'pages/user/submissions' %>
			</div>

			<%# render 'pages/user/dashboard' %>

		</div>

	<% else %>

		<%= render 'pages/user/not_artist_home' %>

	<%end%>

<%end%>