<p id="notice"><%= notice %></p>

<h1>Listing Item Arts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Description</th>
      <th>Name</th>
      <th>Height</th>
      <th>Width</th>
      <th>Length</th>
      <th>Venue</th>
      <th>Price</th>
      <th>Search code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_arts.each do |item_art| %>
      <tr>
        <td><%= item_art.user_id %></td>
        <td><%= item_art.description %></td>
        <td><%= item_art.name %></td>
        <td><%= item_art.height %></td>
        <td><%= item_art.width %></td>
        <td><%= item_art.length %></td>
        <td><%= item_art.venue_id %></td>
        <td><%= item_art.price %></td>
        <td><%= item_art.search_code %></td>
        <td><%= link_to 'Show', item_art %></td>
        <td><%= link_to 'Edit', edit_item_art_path(item_art) %></td>
        <td><%= link_to 'Destroy', item_art, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item art', new_item_art_path %>
